
public class Main {
	public void merge(int[] n1, int m, int[] n2, int n){
		int[] n3 = new int[m+n];
		
		if(n == 0)return;
		if(m == 0){
			n1 = n2;
			return;
		}
		
		int i = 0,j = 0,k = 0;
		while(i < m && j < n){
			if(n1[i] < n2[j]){
				n3[k++] = n1[i++];
			}
			else{
				n3[k++] = n2[j++];
			}
		}
		
		if(i == m){
			while(j < n){
				n3[k++] = n2[j++];
			}
		}
		else{
			while(i < m){
				n3[k++] = n1[i++];
			}
		}
		n1 = n3;
	}
}
