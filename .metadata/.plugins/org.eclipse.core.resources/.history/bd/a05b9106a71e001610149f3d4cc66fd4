import java.util.ArrayList;

public class Main {
	public boolean isPrimes(int n, ArrayList nums) {
		int i;
		for (i = 0; i < nums.size(); i++)
			if (n % (Integer) nums.get(i) == 0)
				return false;
		return true;
	}

	public int countPrimes(int n) {
		int i, tmp = 1;
		ArrayList nums = new ArrayList();

		for (i = 2; i < n; i++) {
			if (isPrimes(i, nums)) {
				nums.add(i);
			}
		}
		return nums.size();
	}
}
