
public class Main {
	public void main(String[] args){
		int[] a = {3,2,2,3};
		removeElement(a,3);
	}
	
    public int removeElement(int[] nums, int val) {
    	for(int i = 0;i <= nums.length-1; i++){
    		
    		if(nums[i] == val){
    			int j = i;
    			for(int k = i+1;k <= nums.length-1; k++){
    				exchange(nums[i],nums[j]);
    				j++;
    			}
    		}
    		
    	}
    	
    	int res = 0;
    	for(int i:nums){
    		if(i == val) break;
    		res++;
    	}
    	
        return res;
    }
    
    public void exchange(int a, int b){
		a = a ^ b;
		b = b ^ a;
		a = a ^ b;
	}
}
