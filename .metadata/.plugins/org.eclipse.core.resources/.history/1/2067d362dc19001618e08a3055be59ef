
public class Main {
    public ListNode deleteDuplicates(ListNode head) {
        ListNode fast;
        ListNode re = head;
        
        for(;head != null ;head = head.next){ //遍历每一个节点
        	fast = head.next;
        	
        	while(fast != null){
        		
        		while(fast != null && fast.val != head.val)fast = fast.next;
        		
        			if(fast != null && fast.next != null){
        				fast.val=fast.next.val;
        				fast.next =fast.next.next;
        		}
        			
        			else{ 
        				ListNode tmp = head;
        				while(tmp.next != fast) tmp = tmp.next;
        				tmp.next = null;
        		}
        			
        			if(fast.next == null)break;
        			fast = fast.next;
        	}
        }
        return re;
    }
}
