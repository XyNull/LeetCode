
public class Main {
    // you need to treat n as an unsigned value
	public static void main(String[] args){
		int a = 2147483647;
		a *= 2;
		a -= 1;
		hammingWeight(a);
	}
	
    public static int hammingWeight(int n) {
        int res = 0;
        while(n != 0){
        	res += n & 1;
        	n >>= 1;
        	if(n == -1)n++;
        }
        return res;
    }
}
