
public class Main {
    // you need to treat n as an unsigned value
	public static void main(String[] args){
		Integer a = 1073741824;
		a *= 2;
		hammingWeight(a);
	}
	
    public static int hammingWeight(int n) {
        int res = 0;
        if(n < 0) res -= 1;
        while(n != 0 && n != -1){
        	res += (n & 1);
        	n >>= 1;
        }
        return res;
    }
}
