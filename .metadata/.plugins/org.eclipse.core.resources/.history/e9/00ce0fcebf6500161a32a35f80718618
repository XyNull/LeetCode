
public class Main {
    // you need to treat n as an unsigned value
	public static void main(String[] args){
		Integer a = 33554432;
		a *= 64;
		hammingWeight(a);
	}
	
    public static int hammingWeight(int n) {
        int res = 0;
        while(Math.abs(n) > 0){
        	if((n & 1) == 1) res++;
        	n = n >> 1;
        }
        return res;
    }
}
