
public class Main {
	public boolean isPrimes(int n, int[] nums) {
		int i;
		for (i = 0; i < nums.length; i++)
			if (n % nums[i] != 0)
				return false;
		return true;
	}

	public int countPrimes(int n) {
		int i = 0, tmp = 1;
		int[] nums = { 2 };

		for (i = 2; i < n; i++) {
			if (!isPrimes(i, nums)) {
				i = nums[tmp];
				tmp++;
			}
		}
		return nums.length;
	}
}
