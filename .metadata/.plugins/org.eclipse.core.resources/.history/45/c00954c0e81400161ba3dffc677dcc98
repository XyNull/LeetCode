
public class Main {
	
	public static void main(String[] args) {
		int[] a={1,0,7,0,2} ;
		moveZeroes(a);
	}
	
    public static void moveZeroes(int[] nums) {
        int fast = 0;
        int slow = 0;
        
        if(nums != null){
        while(fast >=0 || slow <= 0){
        	while(nums[fast] != 0) fast++;
    		while(nums[slow]== 0 ) slow++;
    		
    		nums[fast] ^= nums[slow];
    		nums[slow] ^= nums[fast];
    		nums[fast] ^= nums[slow];

        	}
        }
    }
}