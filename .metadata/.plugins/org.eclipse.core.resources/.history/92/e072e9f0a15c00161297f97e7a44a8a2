
public class Main {
    public int threeSumClosest(int[] n, int target) {
    	int length = n.length;
    	if(length <= 3) return n[0]+n[1]+n[2];
    	
    	Arrays.sort(n);
    	
loopi:   for(int i = 0; i < length; i++){ 	
        	int p = i+1,q = p+1;
        	
loopp:    	for(;p <= length-2; p++){
				q = p+1;
loopq:			while(q <= length-1){
    				if(n[q]+n[p]+n[i] == 0){
    				}
    				if(q == length-1) break;
    				q++;
        		}
        	}
        	if(i >= length-1) break;
        }
        
        return;
    }
}
