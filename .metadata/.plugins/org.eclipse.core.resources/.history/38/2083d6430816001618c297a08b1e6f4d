import java.util.ArrayList;
import java.util.List;

public class Main {
	public List<Integer> preorderTraversal(TreeNode root) {
		List resu = new ArrayList();
		if(root==null) return null;
		resu.add(root.val);
		
		resu.add(preorderTraversal(root.left));
		if(root.left != null) resu.add(root.left.val);
		
		resu.add(preorderTraversal(root.right));
		if(root.right != null) resu.add(root.right.val);
		
		return resu;
	}
}