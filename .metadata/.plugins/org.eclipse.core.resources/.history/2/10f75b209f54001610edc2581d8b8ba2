
public class Better {
	public static RandomListNode copyNode(RandomListNode head){
		if(head == null) return null;
		
		RandomListNode copy = new  RandomListNode(head.label), 
				output = copy;
		head = head.next;
		
		while(head != null){
			copy.next = new RandomListNode(head.label);
			copy = copy.next;
			head = head.next;
		}
		
		return output;
	}
	
	
}
