import java.util.ArrayList;
import java.util.List;

public class Main {
    public List<List<Integer>> levelOrderBottom(TreeNode root) {
    	if(root == null) return null;
    	
    	List<List<Integer>> valGroup = new ArrayList<List<Integer>>();
    	List<Integer> vals = new ArrayList<Integer>();
    	
    	vals.add(root.val);
    	
    	if(root.left != null)
    		vals.addAll(helper(root.left));
    	
    	if(root.right != null)
    		vals.addAll(helper(root.right));
    	
    	valGroup.add(vals);
    	return valGroup;
    }
    
    public List<Integer> helper(TreeNode root){
    	
    }
}
