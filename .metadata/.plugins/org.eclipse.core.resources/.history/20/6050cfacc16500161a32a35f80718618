
public class Main {
    // you need to treat n as an unsigned value
	public static void main(String[] args){
		long a = 1073741824;
		a *= 2;
		hammingWeight(a);
	}
	
    public static int hammingWeight(long n) {
        int res = 0;
        while(n != 0){
        	res += (n & 1);
        	n >>= 1;
        }
        return res;
    }
}
